test -f /etc/vendor.inf && . /etc/vendor.inf
export PATH=/sbin:/bin:/usr/bin:/usr/sbin
echo Welcome to the inst-sys...
uname -a
pwd
PS1="inst-sys:\w # "
alias -- +='pushd .'
alias -- -='popd'
alias ..='cd ..'
alias ...='cd ../..'
alias dir='ls -l'
alias ls-l='ls -l'
alias l='ls -lFa'
alias o='less'

echo
echo "run `cat /tmp/linuxrc_installer_name` to start the installation"
echo
