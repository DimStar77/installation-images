echo 'This is SuSE Instsys Version %{version} Rel. %{release}'
echo 'Build date: Tue Aug  7 14:49:34 CEST 2001'
PATH=/sbin:/bin:/usr/sbin:/usr/bin
. /bin/netsetup
dmesg > /kernel.msg
echo
echo "You should be able to login via telnet/ssh now."
echo "To restart network setup, enter:"
echo "netsetup"
echo
while bash -login
do echo "linuxrc $$: restarting bash"; sleep 1; done
