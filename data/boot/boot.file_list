ifarch ix86

<kernel_rpm>:
m /boot/<kernel_img> /linux

ifenv demo yes
x message-demo /message
x syslinux-demo.cfg /syslinux.cfg
else
ifenv demo old
x message-demoold /message
x syslinux-demoold.cfg /syslinux.cfg
else
x message /
x syslinux.cfg /
endif
endif


ifarch alpha

<kernel_rpm>:
m /boot/<kernel_img> /vmlinux.gz

endif
